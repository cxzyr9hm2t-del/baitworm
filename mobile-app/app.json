{
  "expo": {
    "name": "Baitworm Canada",
    "slug": "baitworm-canada",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "baitworm",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#d52b1e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ca.baitworm.app",
      "buildNumber": "1.1.0",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Baitworm Canada uses your location to find nearby fishing spots in Ontario.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#d52b1e"
      },
      "package": "ca.baitworm.app",
      "versionCode": 2,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-location"
    ],
    "description": "Baitworm Canada - Kingston, Ontario's premium live bait worm farm. Order fresh Canadian nightcrawlers, red wigglers, and more. Find fishing spots across Ontario.",
    "githubUrl": "https://github.com/cxzyr9hm2t-del/baitworm",
    "owner": "baitworm-canada",
    "extra": {
      "eas": {
        "projectId": "baitworm-canada"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
